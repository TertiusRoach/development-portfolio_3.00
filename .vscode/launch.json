{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Chrome Console",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "node",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "name": "Go Webpack",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run-script", "public"]
    }
  ],
  "compounds": [
    {
      "name": "Run Application",
      "configurations": ["Go Webpack", "Chrome Console"]
    }
  ]
}
